<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeCoursesFragment">

    <fragment
        android:id="@+id/homeCoursesFragment"
        android:name="com.education.brainbeast.ui.education.ui.menuhome.HomeCoursesFragment"
        android:label="fragment_home_courses"
        tools:layout="@layout/fragment_home_courses" >
        <action
            android:id="@+id/action_homeCoursesFragment_to_coursesStaggedFragment"
            app:destination="@id/coursesStaggedFragment" />
        <action
            android:id="@+id/action_homeCoursesFragment_to_searchMatchesCoursesFragment"
            app:destination="@id/searchMatchesCoursesFragment" />
    </fragment>
    <fragment
        android:id="@+id/coursesStaggedFragment"
        android:name="com.education.brainbeast.ui.education.ui.menucourses.CoursesStaggedFragment"
        android:label="fragment_courses_stagged"
        tools:layout="@layout/fragment_courses_stagged" />
    <fragment
        android:id="@+id/searchMatchesCoursesFragment"
        android:name="com.education.brainbeast.ui.education.ui.menusearch.MatchesCoursesFragment"
        android:label="fragment_matches_courses"
        tools:layout="@layout/fragment_matches_courses" />
    <fragment
        android:id="@+id/joinVideoCallFragment"
        android:name="com.education.brainbeast.ui.education.ui.menujoinvideoconference.JoinVideoConferenceFragment"
        android:label="fragment_matches_courses"
        tools:layout="@layout/fragment_join_video_meeting" />
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.education.brainbeast.ui.education.ui.menuprofile.UserProfileFragment"
        android:label="user_profile_fragment"
        tools:layout="@layout/fragment_user_profile" />
</navigation>